{
  "extends": "expo/tsconfig.base.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"]
    },
    "types": ["expo-router"],
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
